{
  "name": "ssr-proxy-js",
  "version": "1.0.0",
  "description": "Server-Side Rendering Proxy",
  "keywords": [
    "ssr",
    "proxy",
    "spa"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Tpessia/ssr-proxy-js"
  },
  "author": "Thiago Pessia",
  "license": "MIT",
  "main": "dist/ssr-proxy.js",
  "types": "dist/ssr-proxy.d.ts",
  "scripts": {
    "setup": "npm install",
    "setup:prod": "npm install",
    "start": "rimraf dist && cross-env NODE_ENV=development SSR_PROXY_ROUTE=localhost:3000 SSR_PROXY_STATIC=../../../build/ webpack --config webpack.config.js --mode development --watch",
    "start:prod": "cross-env NODE_ENV=production SSR_PROXY_ROUTE=localhost:81 SSR_PROXY_STATIC=../../../build/ node dist/index.js",
    "build": "rimraf dist && cross-env NODE_ENV=production webpack --config webpack.config.js --mode production",
    "http-test": "node http-test.js"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "deepmerge": "^4.2.2",
    "express": "^4.17.1",
    "isbot": "^3.3.4",
    "mime-types": "^2.1.34",
    "puppeteer": "^10.4.0",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/mime-types": "^2.1.1",
    "cross-env": "^7.0.3",
    "nodemon-webpack-plugin": "^4.5.2",
    "rimraf": "^3.0.2",
    "ts-loader": "^9.2.6",
    "typescript": "^4.4.4",
    "webpack": "^5.62.1",
    "webpack-cli": "^4.9.1",
    "webpack-node-externals": "^3.0.0"
  }
}
